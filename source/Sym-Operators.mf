
for scale = 1, 1.4:
  beginbigop(scale); "big plus";
    pickup rule_pen;
    pickup currentpen scaled (4/3 * sqrt scale);

    draw (centre + op_height/2 * dir  0) -- (centre + op_height/2 * dir 180);
    draw (centre + op_height/2 * dir 90) -- (centre + op_height/2 * dir 270);
